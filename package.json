  ],
  "author": "Shankar Ganesh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shangan23/crypto-urls/issues"
  },
  "homepage": "https://github.com/shangan23/crypto-urls#readme",
  "devDependencies": {
    "@babel/cli": "7.13.14",
    "@babel/core": "7.13.15",
    "@babel/preset-env": "7.13.15",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "18.0.0",
    "@rollup/plugin-node-resolve": "11.2.1",
    "babel-jest": "26.6.3",
    "jest": "26.6.3",
    "url-parse": "1.5.1",
    "crypto-js": "4.0.0",
    "query-string": "7.0.0",
